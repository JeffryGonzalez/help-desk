{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "ng-dev",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "ng-dev": {

        "Destinations": {
          "ng-dev/serve": {
            "Address": "http://localhost:4200/"
          }
        }
      }
    }
  }
}
