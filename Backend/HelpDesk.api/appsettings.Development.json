{
  "Authentication": {
    "Schemes": {
      "oidc": {
        "Authority": "http://localhost:9999/default",
        "ClientId": "default",
        "ClientSecret": "secret",
        "ResponseType": "code",
        "ResponseMode": "query",
        "GetClaimsFromUserInfoEndpoint": true,
        "MapInboundClaims": false,
        "SaveTokens": true,
        "Scope": "openid profile offline_access",
        "RequireHttpsMetadata": false,
        "TokenValidationParameters": {
          "NameClaimType": "name",
          "RoleClaimType": "role"
        }
      },
      "cookie": {
       
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "ng-dev",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "ng-dev": {

        "Destinations": {
          "ng-dev/serve": {
            "Address": "http://localhost:4200/"
          }
        }
      }
    }
  }
}
